[project]
	description = Source files for Xperia U
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Info4Geek
[access "refs/for/refs/*"]
	read = group user/Info4Geek
	push = group user/Info4Geek
	label-Code-Review = -2..+2 group user/Info4Geek
	label-Verified = -1..+1 group user/Info4Geek
	rebase = group user/Info4Geek
	removeReviewer = group user/Info4Geek
	submit = group user/Info4Geek
[access "refs/heads/*"]
	read = group user/Info4Geek
	create = group user/Info4Geek
	push = +force group user/Info4Geek
	pushMerge = group user/Info4Geek
[access "refs/tags/*"]
	push = +force group user/Info4Geek
	pushTag = group user/Info4Geek
	pushSignedTag = group user/Info4Geek
